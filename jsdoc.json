{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
  },
  "source": {
      "include": [
        "README.md",
        "src/index.js",
        "src/EksiGuest.js",
        "src/EksiMember.js",
        "src/EksiSozluk.js",
        "src/exceptions"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown"
  ],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": true,
      "useLongnameInNav": false
  },
  "opts": {
      "destination": "./docs/",
      "encoding": "utf8",
      "private": true,
      "recurse": true,
      "template": "./node_modules/minami",
      "tutorials": "./tutorials"
  }
}
