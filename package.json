{
  "name": "eksi-sozluk",
  "version": "5.0.1",
  "description": "Eksi Sozluk NodeJS client.",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:active": "eslint-nibble .",
    "test": "jest --config ./jest.config.js",
    "test:coverage": "npm test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "semantic-release": "semantic-release",
    "docs": "rm -rf docs && jsdoc --configure jsdoc.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ridvanaltun/eksi-sozluk.git"
  },
  "keywords": [
    "ekşi",
    "sözlük",
    "ekşisözlük",
    "eksisozluk"
  ],
  "author": {
    "name": "Rıdvan Altun",
    "email": "ridvanaltun@outlook.com",
    "url": "https://ridvanaltun.github.io/"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ridvanaltun/eksi-sozluk/issues"
  },
  "homepage": "https://github.com/ridvanaltun/eksi-sozluk#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.10",
    "form-data": "^4.0.0",
    "https-proxy-agent": "^5.0.0",
    "moment": "^2.29.1",
    "object-assign-deep": "^0.4.0",
    "querystring": "^0.2.1",
    "set-cookie-parser": "^2.4.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@commitlint/cz-commitlint": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-nibble": "^7.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jsdoc": "^3.6.7",
    "lint-staged": "^11.1.2",
    "minami": "^1.2.3",
    "prettier": "^2.4.1",
    "semantic-release": "^17.4.7"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
